{
    "name": "kunstmaan/lead-generation-bundle",
    "type": "symfony-bundle",
    "description": "This bundle makes it easy to create lead generation popups for your website.",
    "keywords": [
        "kunstmaan",
        "lead generation",
        "leads",
        "popup",
        "modal"
    ],
    "homepage": "https://github.com/Kunstmaan/KunstmaanLeadGenerationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "kunstmaan/admin-bundle": "~5.2",
        "kunstmaan/adminlist-bundle": "~5.2",
        "kunstmaan/node-bundle": "~5.2",
        "kunstmaan/utilities-bundle": "~5.2"
    },
    "require-dev": {
        "codeception/codeception": "^2.4",
        "matthiasnoback/symfony-config-test": "2.2.0",
        "matthiasnoback/symfony-dependency-injection-test": "1.2.0",
        "phpunit/phpunit": "^5.7",
        "symfony/phpunit-bridge": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Kunstmaan\\LeadGenerationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    }
}
